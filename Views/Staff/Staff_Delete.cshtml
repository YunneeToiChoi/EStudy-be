@model study4_be.Models.Staff

<h4>Delete Staff</h4>
<hr />
<div class="card-body">
    <p>Are you sure you want to delete this?</p>
    <div>
        <h4>@Model.StaffCmnd</h4>
        <p>@Model.StaffName</p>
        <p>@Model.StaffType</p>
        <p>@Model.StaffEmail</p>
    </div>
    <form asp-action="Staff_DeleteConfirmed">
        <input type="hidden" name="id" value="@Model.StaffCmnd" />
        <button type="submit" class="btn btn-danger">Delete</button>
        @Html.ActionLink("Cancel", "Staff_List", null, new { @class = "btn btn-light" })
    </form>
    <div asp-validation-summary="All" class="text-danger"></div>
</div>
