@model study4_be.Models.Video

<h4>Delete Video</h4>
<hr />
<div class="card-body">
    <p>Are you sure you want to delete this?</p>
    <div>
        <h4>@Model.VideoId</h4>
        <h4>@Model.VideoUrl</h4>
    </div>
    <form asp-action="Video_Delete">
        <input type="hidden" asp-for="VideoId" />
        <button type="submit" class="btn btn-danger">Delete</button>
        @Html.ActionLink("Cancel", "Video_List", null, new { @class = "btn btn-light" })
    </form>
    <div asp-validation-summary="All" class="text-danger"></div>
</div>